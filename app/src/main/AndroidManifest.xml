<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.cockounter">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".ResumeGameActivity">
        </activity>
        <activity android:name=".StartSinglePlayerGameActivity">
        </activity>
        <activity android:name=".AdminGameScreenActivity">
        </activity>
        <activity android:name=".PlayerGameScreenActivity">
        </activity>
        <activity android:name=".EditScriptActivity">
        </activity>
        <activity android:name=".EditRoleActivity">
        </activity>
        <activity android:name=".EditParameterActivity">
        </activity>
        <activity android:name=".EditPresetActivity">
        </activity>
        <activity android:name=".SelectPresetActivity">
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>